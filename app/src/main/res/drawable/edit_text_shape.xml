    android:thickness="0dp"
    android:shape="rectangle">
    <stroke android:width="3dp"
        android:color="#FAFAFA"/>
    <corners android:radius="5dp" />
    <gradient
        android:startColor="#F1F1FA"
        android:endColor="#FFFFFF"
        android:type="linear"
        android:angle="270"/>
</shape>
